            {
                "nombre": "actor",
                "codigo": "// @ts-ignore\nclass actor extends Actor {\n    contador_de_disparo: number;\n\n    iniciar() {\n        this.animacion = \"navePlayer\";\n        this.contador_de_disparo = 0;\n    }\n\n    actualizar() {\n        let velocidad = 5;\n\n        if (this.pilas.control.izquierda) {\n            this.x -= velocidad;\n\n            if (this.x < -230) {\n                this.x = -230\n            }\n        }\n\n        if (this.pilas.control.derecha) {\n            this.x += velocidad;\n\n            if (this.x > 230) {\n                this.x = 230\n            }\n        }\n\n        if (this.pilas.control.abajo) {\n            this.y -= velocidad;\n\n            if (this.y < -230) {\n                this.y = -230\n            }\n        }\n\n        if (this.pilas.control.arriba) {\n            this.y += velocidad;\n\n            if (this.y > 200) {\n                this.y = 200;\n            }\n        }\n\n        if (this.pilas.control.espacio) {\n            if (this.contador_de_disparo < 0) {\n                this.disparar();\n                this.contador_de_disparo = 5;\n            } else {\n                this.contador_de_disparo -= 1;\n            }\n        }\n    }\n\n    disparar() {\n        let disparo = this.pilas.clonar_en(\"disparo\", this.x, this.y + 20);\n    }\n\n}"
            },
            {
                "nombre": "disparo",
                "codigo": "// @ts-ignore\nclass disparo extends Actor {\n\n    velocidad;\n\n    iniciar() {\n        this.centro_x = 0.3;\n        this.velocidad = 10;\n        this.pilas.reproducir_sonido(\"laser\");\n    }\n\n    actualizar() {\n        this.avanzar(this.y + 10, this.velocidad);\n        this.eliminar_si_sale_de_la_pantalla();\n    }\n\n    eliminar_si_sale_de_la_pantalla() {\n        let izquierda = this.pilas.camara.borde_izquierdo;\n        let derecha = this.pilas.camara.borde_derecho;\n        let arriba = this.pilas.camara.borde_arriba;\n        let abajo = this.pilas.camara.borde_abajo;\n\n        if (this.x > derecha || this.x < izquierda || this.y > arriba || this.y < abajo) {\n            this.eliminar();\n        }\n    }\n}"
            },
            {
                "nombre": "balaPlayer",
                "codigo": "// @ts-ignore\nclass balaPlayer extends Actor {\n\n    velocidad;\n\n    iniciar() {\n        this.centro_x = 0.3;\n        this.velocidad = 10;\n        this.pilas.reproducir_sonido(\"laser\");\n    }\n\n    actualizar() {\n        this.avanzar(this.x + 10, this.velocidad);\n        this.eliminar_si_sale_de_la_pantalla();\n    }\n\n    eliminar_si_sale_de_la_pantalla() {\n        let izquierda = this.pilas.camara.borde_izquierdo;\n        let derecha = this.pilas.camara.borde_derecho;\n        let arriba = this.pilas.camara.borde_arriba;\n        let abajo = this.pilas.camara.borde_abajo;\n\n        if (this.x > derecha || this.x < izquierda || this.y > arriba || this.y < abajo) {\n            this.eliminar();\n        }\n    }\n}"
