            {
                "nombre": "escenaTransicion",
                "codigo": "class escenaTransicion extends Escena {\n\n  iniciar() {\n\n  }\n\n  actualizar() {\n    this.pilas.luego(2, () => {\n      if (this.proyecto.estado_juego == \"ganaste\") {\n        if (this.proyecto.numero_escena == 2) this.pilas.cambiar_escena(\"escena2\"); else\n        if (this.proyecto.numero_escena == 3) this.pilas.cambiar_escena(\"escena3\"); else {\n            this.pilas.cambiar_escena(\"Menu\");\n        }\n\n      } else {\n        this.pilas.cambiar_escena(\"Menu\");\n      }\n    }\n    );\n  }\n\n}"
            },
            {
                "nombre": "escena3",
                "codigo": "class escena3 extends Escena {\n\n  iniciar() {\n    for (let i = 0; i < 10; i++) {\n      this.pilas.clonar_en_posicion_al_azar(\"moneda\");\n    }\n\n    for (let i = 0; i < 30; i += 5) {\n      this.pilas.clonar_en_posicion_al_azar(\"plataforma\");\n    }\n\n    //CREAR AL JUGADOR\n    this.pilas.clonar_en(\"TRN\", 5.72, -307);\n    this.pilas.clonar_en(\"barra_de_energia\", 200, 350);\n    this.pilas.clonar_en(\"llave\", 200, 300);\n\n    //NUMERO ESCENA\n    //this.proyecto.numero_escena = 2;\n  }\n\n  actualizar() {\n\n  }\n\n  cuando_llega_el_mensaje_llave_obtenida() {\n    this.proyecto.llave_obtenida = true;\n  }\n\n}"
            }
