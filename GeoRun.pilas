                "codigo": "// @ts-ignore\nclass plataforma extends Actor {\n  iniciar() {\n\n  }\n}"
            },
            {
                "nombre": "caja",
                "codigo": "// @ts-ignore\nclass caja extends Actor {\n\n  posX1 = 0;\n  posX2 = -100;\n  posX3 = 100;\n  contador = 0;\n\n  iniciar() {\n\n  }\n\n\n\n  actualizar() {\n    //let velocidad = 5;\n\n    if (this.contador == -1) { this.x = -100 } else\n      if (this.contador == 0) { this.x = 0 } else\n        if (this.contador == 1) { this.x = 100 }\n\n\n\n    \n        if (this.pilas.control.izquierda) {\n          if(this.contador == 0){this.contador = -1;}else\n          if(this.contador == 1){this.contador = 0;}\n          //this.contador--;\n          \n        }else\n        if (this.pilas.control.derecha) {\n          if(this.contador == 0){this.contador = 1;}else\n          if(this.contador == -1){this.contador = 0;}\n          //this.contador++;\n          \n        }\n        \n\n\n\n    this.pilas.observar(\"contador\", this.contador);\n\n\n  }\n\n\n}"
            },
            {
                "nombre": "TRN",
                "codigo": "// @ts-ignore\nclass TRN extends Actor {\n\n  velocidad = 0;\n  limiteVelocidadY = 2;\n  cuadros_desde_el_ultimo_disparo: number;\n  DERECHA = 0; IZQUIERDA = 180;\n  velocidadHorizontal = 0;\n  //gravedad = 15;\n  estado_nave: string;\n  anchoPantalla: number;\n  altoPantalla: number;\n\n  iniciar() {\n    this.animacion = \"nave_en_reposo\";\n    this.cuadros_desde_el_ultimo_disparo = 0;\n    //this.dinamico = false;\n    this.estado_nave = \"reposo\";\n    //Obtenemos el limite del recorrido de nuestro personaje en el eje X\n    this.anchoPantalla = this.pilas.camara.borde_derecho;\n    this.altoPantalla = this.pilas.camara.borde_arriba;\n\n  }\n\n  actualizar() {\n    this.cuadros_desde_el_ultimo_disparo += 1;\n    //this.avanzar(270, this.gravedad);\n    //this.cuando_colisiona();\n\n    //IMPULSO----------------------------------------------------------------\n    if (this.pilas.control.arriba) {\n      this.animacion = \"nave_avanzando\";\n      this.estado_nave = \"volando\";\n      this.proyecto.estado_juego = \"jugando\";\n\n      this.velocidad += 0.1;\n      this.avanzar(this.rotacion, this.velocidad);\n\n      //Controla la velocidad\n      if (this.velocidad > this.limiteVelocidadY) {\n        this.velocidad = this.limiteVelocidadY;\n      }\n\n      //Cuando se pulsa la tecla arriba se anula la gravedad en Y\n      this.pilas.fisica.gravedad_y = 0;\n    } else {\n      if (!this.pilas.control.izquierda && !this.pilas.control.derecha) {\n        this.animacion = \"nave_en_reposo\";\n        //Cancela la velocidad horizontal\n        this.velocidadHorizontal = 0;\n      }\n      //Si no se presiona la tecla arriba la gravedad en Y ser√° 0.5\n      //Y la velocidad vuelve a cero\n      this.pilas.fisica.gravedad_y += 0.01;\n      this.velocidad = 0;\n    }\n\n    //MOVIMIENTO HORIZONTAL--------------------------------------\n    if (this.estado_nave == \"volando\") {\n      if (this.pilas.control.izquierda) {\n        //if (this.rotacion != 90) { this.rotacion += this.velocidad; }\n        //this.rotacion += this.velocidad;\n        this.velocidadHorizontal = 3;\n        this.avanzar(this.IZQUIERDA, this.velocidadHorizontal / 2);\n        this.animacion = \"nave_girando_a_la_izquierda\";\n      }\n\n      if (this.pilas.control.derecha) {\n        //if (this.rotacion != 90) { this.rotacion -= this.velocidad; }\n        //this.rotacion -= this.velocidad;\n        this.velocidadHorizontal = 3;\n        this.avanzar(this.DERECHA, this.velocidadHorizontal / 2);\n        this.animacion = \"nave_girando_a_la_derecha\";\n      }\n\n    }\n\n    this.control_limites_pantalla();\n\n  }\n\n  control_limites_pantalla () {\n    if (this.x > this.anchoPantalla - this.figura_ancho ){this.x = this.anchoPantalla - this.figura_ancho} else\n    if (this.x < -this.anchoPantalla + this.figura_ancho){ this.x = -this.anchoPantalla + this.figura_ancho}else\n\n    if ( this.y > this.altoPantalla) {this.y = this.altoPantalla}else\n    if ( this.y < -this.altoPantalla) {\n      //this.proyecto.estado_juego = \"perdiste\";\n      //this.eliminar();\n      //this.pilas.cambiar_escena(\"escenaTransicion\");\n      //this.enviar_mensaje_global(\"nave_perdida\");\n    }\n  }\n\n  cuando_comienza_una_colision(actor: Actor) {\n    //Cuando colisiona con el suelo\n    if (actor.tiene_etiqueta(\"plataforma\")) {\n      //this.tipoArma++;\n      //this.reproducir_sonido(\"moneda\");\n      this.enviar_mensaje_global(\"colision_plataforma\");//Este mensaje lo recibe el actor barra_de_energia\n      //this.eliminar();\n      //this.dinamico = false;\n      this.estado_nave = \"reposo\";\n\n      if (this.proyecto.estado_juego == \"jugando\") {\n        let explosion = this.pilas.actores.explosion();\n        explosion.escala = 0.2;\n        explosion.x = this.x;\n        explosion.y = this.y;\n      }\n    }\n\n    if (actor.tiene_etiqueta(\"moneda\")) {\n      this.reproducir_sonido(\"moneda\");\n      actor.eliminar();\n    }\n\n    if (actor.tiene_etiqueta(\"llave\")) {\n      this.reproducir_sonido(\"moneda\");\n      //actor.escala +=0.3;\n      actor.eliminar();\n      this.enviar_mensaje_global(\"llave_obtenida\");//Este mensaje lo recibe escena1\n      this.decir(\"Llave Obtenida, Regrese a la base\", 3, -10);\n    }\n\n    //AQUI SE DEFINE SI PASA A LA SIGUIENTE ESCENA O REINICIA EL JUEGO\n    if (actor.tiene_etiqueta(\"base\")) {\n      if (this.proyecto.llave_obtenida) {\n        //this.pilas.cambiar_escena(\"escena2\");\n        this.proyecto.numero_escena++;\n        this.proyecto.estado_juego = \"ganaste\";\n        this.pilas.cambiar_escena(\"escenaTransicion\");\n      }\n      \n    }\n\n  }\n\n}\n"
            },
            {
                "nombre": "barra_de_energia",
                "codigo": "// @ts-ignore\nclass barra_de_energia extends PizarraBase {\n\n  barra_largo: number = 200;\n  barra_alto: number = 15;\n  vida_actual: number; // valor entre 0 y 100\n\n  private vida_anterior: number = 0;\n\n  iniciar() {\n    super.iniciar();\n    this.imagen = \"imagenes:basicos/invisible\";\n    this.vida_actual = this.proyecto.vida_actual;\n  }\n\n  actualizar() {\n    this.dibujar();\n    //this.vida_actual -= 0.2;\n  }\n\n  cuando_llega_el_mensaje_colision_plataforma() {\n    //Evita que le reste vida antes de que presione la tecla arriba\n    if (this.proyecto.estado_juego == \"jugando\"){ this.vida_actual -= 25;} //Resta energia\n\n    if (this.vida_actual <= 0) {\n      //Eliminamos nuestra nave\n      let player = this.pilas.obtener_actor_por_nombre(\"TRN\");\n      player.eliminar();\n      this.proyecto.estado_juego = \"perdiste\";\n      this.pilas.cambiar_escena(\"escenaTransicion\");\n      \n\n      //this.pilas.enviar_mensaje_global(\"energia_cero\");\n    }\n  }\n\n\n  dibujar() {\n    if (this.vida_actual === this.vida_anterior) {return;}\n\n    this.vida_actual = Math.max(0, this.vida_actual);\n    this.vida_actual = Math.min(100, this.vida_actual);\n    this.vida_anterior = this.vida_actual;\n    this.dibujar_barra();\n  }\n\n  private dibujar_barra() {\n    let vida = this.vida_actual;\n    let largo = this.barra_largo;\n    let alto = this.barra_alto;\n    let x = -largo / 2;\n    let y = +alto / 2;\n\n    this.limpiar();\n    this.dibujar_borde_de_rectangulo(x, y, largo, alto, \"blanco\", 4);\n    this.dibujar_rectangulo(x, y, largo, alto, \"rojo\");\n\n    this.dibujar_rectangulo(x, y, (largo / 100) * vida, alto, \"amarillo\");\n  }\n}\n"
            },
