                "codigo": "// @ts-ignore\nclass nave extends Actor {\n\n    anchoPantalla\n    altoPantalla\n    estadoJugador\n\n    iniciar() {\n        this.estadoJugador = \"despegue\";\n    }\n\n\n    limite_pantalla() {\n        let ancho_pantalla = this.pilas.camara.borde_derecho - this.figura_ancho / 2;\n        let alto_pantalla = this.pilas.camara.borde_arriba - this.figura_alto / 2;\n\n        if (this.x < -ancho_pantalla) this.x = -ancho_pantalla;//lado izquierdo\n        if (this.x > ancho_pantalla) this.x = ancho_pantalla;//lado derecho\n\n        if (this.y > alto_pantalla) this.y = alto_pantalla;//arriba\n        if (this.y < -alto_pantalla) this.y = -alto_pantalla;//abajo\n\n    }\n\n    cuando_hace_click_en_la_pantalla(x: number, y: number, evento) {\n        this.impulsar(0, 10);\n        //this.pilas.reproducir_sonido(\"salto-corto\");\n    }\n\n    movimiento() {\n        if (this.estadoJugador == \"despegue\") {\n            let velocidad = 5;\n            let velocidad_para_doblar = 0.5;\n\n            if (this.pilas.control.arriba) {\n                this.rotacion += this.pilas.azar(-0.05, 0.05);\n                //this.avanzar(this.rotacion, velocidad)\n                this.impulsar(0, 10);\n            }\n\n            if (this.pilas.control.tecla_a) {\n                this.rotacion += velocidad_para_doblar;\n            }\n\n            if (this.pilas.control.tecla_d) {\n                this.rotacion -= velocidad_para_doblar;\n            }\n        }\n    }\n\n\n\n    actualizar() {\n        this.limite_pantalla();\n        //this.rotacion = this.velocidad_y * 5;\n        this.movimiento();\n\n\n\n    }\n\n\n\n\n\n\n\n}"
