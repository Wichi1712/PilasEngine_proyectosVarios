            {
                "nombre": "dado2",
                "codigo": "// @ts-ignore\nclass dado2 extends Actor {\n //contador;\n //velocidad_x;\n\n    anchoPantalla;\n    altoPantalla;\n\n    iniciar() {\n        //this.contador = 0;\n        this.velocidad_x = 2;\n        this.velocidad_y = 2;\n\n        //*****Obtenemos el limite del recorrido de nuestro personaje en el eje X e Y\n        //this.anchoPantalla = this.pilas.camara.borde_derecho - this.figura_ancho/2;\n        //this.altoPantalla = this.pilas.camara.borde_arriba - this._figura_alto/2;\n    }\n\n    limite_pantalla() {\n        let ancho_pantalla = this.pilas.camara.borde_derecho - this.figura_ancho/2;\n        let alto_pantalla = this.pilas.camara.borde_arriba - this._figura_alto/2;\n        //let ancho_imagen = this.figura_ancho/2;\n        //let alto_imagen = this._figura_alto /2;\n\n        if (this.x < -ancho_pantalla) this.x = -ancho_pantalla;//lado izquierdo\n        if (this.x > ancho_pantalla) this.x = ancho_pantalla;//lado derecho\n  \n        if (this.y > alto_pantalla) this.y = alto_pantalla;//arriba\n        if (this.y < -alto_pantalla) this.y = -alto_pantalla;//abajo\n\n    }\n\n\n\n\n    actualizar() {\n        this.limite_pantalla();\n        this.entrada();\n        \n\n    }\n\n\n\n\n    entrada() {\n        let velocidad = 5;\n\n        if (this.pilas.control.izquierda) {\n            this.x -= velocidad;\n            //this.contador = 1;\n            //this.decir(\"ESte es el case 1\");\n        }\n\n        if (this.pilas.control.derecha) {\n            this.x += velocidad;\n            //this.contador = 2;\n            //this.decir(\"ESte es el case 2\");\n        }\n\n        if (this.pilas.control.arriba) {\n            this.y += velocidad;\n            //this.contador++;\n        }\n\n        if (this.pilas.control.abajo) {\n            this.y -= velocidad;\n            //this.contador--;\n        }\n    }\n\n\n\n}"
            },
            {
                "nombre": "dado4",
                "codigo": "// @ts-ignore\nclass dado4 extends Actor {\n contador;\n velocidad_x;\n\n    anchoPantalla;\n    altoPantalla;\n\n    iniciar() {\n        //this.imagen = \"imagenes:objetos/aceituna\";\n        this.contador = 0;\n        this.velocidad_x = 2;\n        this.velocidad_y = 2;\n\n        //Obtenemos el limite del recorrido de nuestro personaje en el eje X\n        this.anchoPantalla = this.pilas.camara.borde_derecho - this.figura_ancho/2;\n\n        this.altoPantalla = this.pilas.camara.borde_arriba - this._figura_alto/2;\n    }\n\n    limite_pantalla() {\n        let ancho_pantalla = this.pilas.camara.borde_derecho - this.figura_ancho/2;\n        let alto_pantalla = this.pilas.camara.borde_arriba - this._figura_alto/2;\n        //let ancho_imagen = this.figura_ancho/2;\n        //let alto_imagen = this._figura_alto /2;\n\n        this.pilas.observar(\"figuraAncho\", this.anchoPantalla);\n\n        if (this.x < -ancho_pantalla) this.x = -ancho_pantalla;//lado izquierdo\n        if (this.x > ancho_pantalla) this.x = ancho_pantalla;//lado derecho\n  \n        if (this.y > alto_pantalla) this.y = alto_pantalla;//arriba\n        if (this.y < -alto_pantalla) this.y = -alto_pantalla;//abajo\n\n        /*\n        if (this.x < -ancho_pantalla + ancho_imagen) this.x = -ancho_pantalla + ancho_imagen;//lado izquierdo\n        if (this.x > ancho_pantalla  - ancho_imagen) this.x = ancho_pantalla - ancho_imagen;//lado derecho\n  \n        if (this.y > alto_pantalla  - alto_imagen) this.y = alto_pantalla  - alto_imagen;//arriba\n        if (this.y < -alto_pantalla + alto_imagen) this.y = -alto_pantalla + alto_imagen;//abajo\n        */\n    }\n\n\n    condicional_switch(){\n         /*\n                switch(this.contador){\n                    case 0:\n                        this.avanzar(30,3);\n                    break;\n        \n                    case 1:\n                        this.avanzar(90,3);\n                        this.decir(\"ESte es el case 1\");\n                    break;\n        \n                    case 2:\n                        this.decir(\"ESte es el case 2\");\n                    break;\n        \n                }\n                */\n    }\n\n    debug(){\n        this.pilas.observar(\"contador\", this.contador);\n\n        //if (this.x < -200) {this.x = -200;}\n        //if (this.x > 200) { this.x = 200; }\n        this.pilas.observar(\"X\", this.x);\n        this.pilas.observar(\"Y\", this.y);\n\n         this.pilas.observar(\"Alto\", this.altoPantalla);\n         this.pilas.observar(\"Ancho\", this.anchoPantalla);\n    }\n\n\n    actualizar() {\n\n        //this.getCircleX(1, 10);\n        this.limite_pantalla();\n        this.entrada();\n        this.condicional_switch();\n\n        \n\n        \n        this.debug();\n\n    }\n\n\n\n\n    entrada() {\n        let velocidad = 5;\n        if (this.pilas.control.izquierda) {\n            this.x -= velocidad;\n            this.contador = 1;\n            //this.decir(\"ESte es el case 1\");\n        }\n\n        if (this.pilas.control.derecha) {\n            this.x += velocidad;\n            this.contador = 2;\n            //this.decir(\"ESte es el case 2\");\n        }\n\n        if (this.pilas.control.arriba) {\n            this.y += velocidad;\n            this.contador++;\n        }\n\n        if (this.pilas.control.abajo) {\n            this.y -= velocidad;\n            this.contador--;\n        }\n    }\n\n    clamp(argumento1: string) {\n        let argumento2 = parseInt(argumento1, 10);\n\n        this.x = argumento2;\n        this.y = argumento2;\n        let ancho_pantalla_horizontal = this.pilas.camara.borde_derecho;\n        let alto_pantalla = this.pilas.camara.borde_abajo;\n        let ancho_imagen = this._figura_ancho;\n        let alto_imagen = this._figura_alto;\n\n        //let abajo = this.pilas.camara.borde_abajo;\n\n        //Obtenemos alto de enemigo\n        //let altoEnemigo = this.pilas.obtener_actor_por_nombre(\"rocaCuadrada\")._figura_alto;\n\n        if (this.x == argumento2) {\n            //if(this.x > 0 &&  this.x < ancho_pantalla)\n            if (this.x > 0) this.x == 0;\n            if (this.x > ancho_pantalla_horizontal - ancho_imagen) this.x == ancho_pantalla_horizontal - ancho_imagen;\n        }\n\n        if (this.y == argumento2) {\n            //if(this.x > 0 &&  this.x < ancho_pantalla)\n            if (this.y > 0) this.y == 0;\n            if (this.y > alto_pantalla - alto_imagen) this.y == alto_pantalla - alto_imagen;\n        }\n\n\n    }\n\n    getCircleX(radians, radius) {\n        return Math.cos(radians) * radius;\n    }\n\n\n\n\n}"
