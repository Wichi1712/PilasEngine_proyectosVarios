            {
                "nombre": "aceituna",
                "codigo": "// @ts-ignore\nclass aceituna extends Actor {\n    \n    \n    velocidad;\n    aumentarTraslacion;\n\n  iniciar() {\n    this.imagen = \"imagenes:objetos/aceituna\";\n    this.velocidad = 0;\n    this.aumentarTraslacion = 0;\n  }\n\n  actualizar(){\n      let nave = this.pilas.obtener_actor_por_nombre(\"navePlayer2\");\n      let angulo = this.pilas.obtener_angulo_entre_actores(nave,this);\n      let traslacion = angulo + this.aumentarTraslacion;\n      let distancia = this.pilas.obtener_distancia_entre_actores(nave,this)\n      this.velocidad =0.2;\n      this.aumentarTraslacion++;\n      //this.x = nave.x + angulo + this.velocidad;\n      //this.y = nave.y + angulo + this.velocidad;\n\n      //this.x = nave.x + traslacion;\n      //this.y = nave.y + traslacion;\n\n      //this.x++;// = nave.x + 50;\n      //this.y++;// = nave.y + 50;\n      \n      //this.avanzar(nave.x + 1, this.velocidad);\n      //this.avanzar(nave.y + 1, this.velocidad);\n\n      \n    \n        //Esta linea podria usarlo para hacer seguimiento al jugador\n      this.hacer_recorrido([nave.x ,nave.y,20,20],0.2,5,true);\n      \n\n      this.pilas.observar(\"angulo\", angulo);\n      this.pilas.observar(\"distancia\", distancia);\n      this.pilas.observar(\"traslacion\", traslacion);\n      this.pilas.observar(\"aumentarTraslacion\", this.aumentarTraslacion);\n      this.pilas.observar(\"x \", this.x);\n      this.pilas.observar(\"y \", this.y);\n\n      //if(distancia > 50) distancia = 50;\n      \n  }\n}\n"
            },
            {
                "nombre": "cabezaGusano",
                "codigo": "// @ts-ignore\nclass cabezaGusano extends Actor {\n\n\n    contador;\n    iniciar() {\n        //this.imagen = \"imagenes:objetos/aceituna\";\n        this.contador = 0;\n    }\n\n    limites() {\n        let ancho_pantalla_horizontal = this.pilas.camara.borde_derecho;\n        let ancho_pantalla_vertical = this.pilas.camara.borde_abajo;\n        let ancho_imagen = this.figura_radio;\n        let alto_imagen = this._figura_alto;\n\n        this.pilas.observar(\"figuraAncho\", ancho_imagen);\n\n        //if(this.x == argumento2){\n        //if(this.x > 0 &&  this.x < ancho_pantalla)\n        if (this.x < -ancho_pantalla_horizontal) this.x = -ancho_pantalla_horizontal;\n        if (this.x > ancho_pantalla_horizontal  - ancho_imagen) this.x = ancho_pantalla_horizontal - ancho_imagen;\n        //}\n\n        //if(this.y == argumento2){\n        //if(this.x > 0 &&  this.x < ancho_pantalla)\n        if (this.y < 0) this.y == 0;\n        if (this.y > ancho_pantalla_vertical - alto_imagen) this.y == ancho_pantalla_vertical - alto_imagen;\n        //}\n\n\n    }\n\n\n    actualizar() {\n\n        this.getCircleX(1, 10);\n        //this.limites();\n        this.entrada();\n        this.pilas.observar(\"contador\", this.contador);\n\n        //if (this.x < -200) {this.x = -200;}\n        //if (this.x > 200) { this.x = 200; }\n        this.pilas.observar(\"X\", this.x);\n\n\n\n\n\n        /*\n                switch(this.contador){\n                    case 0:\n                        this.avanzar(30,3);\n                    break;\n        \n                    case 1:\n                        this.avanzar(90,3);\n                        this.decir(\"ESte es el case 1\");\n                    break;\n        \n                    case 2:\n                        this.decir(\"ESte es el case 2\");\n                    break;\n        \n                }\n                */\n\n\n    }\n\n    entrada() {\n        let velocidad = 5;\n        if (this.pilas.control.izquierda) {\n            this.x -= velocidad;\n            this.contador = 1;\n            //this.decir(\"ESte es el case 1\");\n        }\n\n        if (this.pilas.control.derecha) {\n            this.x += velocidad;\n            this.contador = 2;\n            //this.decir(\"ESte es el case 2\");\n        }\n\n        if (this.pilas.control.arriba) {\n            this.y += velocidad;\n            this.contador++;\n        }\n\n        if (this.pilas.control.abajo) {\n            this.y -= velocidad;\n            this.contador--;\n        }\n    }\n\n    clamp(argumento1: string) {\n        let argumento2 = parseInt(argumento1, 10);\n\n        this.x = argumento2;\n        this.y = argumento2;\n        let ancho_pantalla_horizontal = this.pilas.camara.borde_derecho;\n        let ancho_pantalla_vertical = this.pilas.camara.borde_abajo;\n        let ancho_imagen = this._figura_ancho;\n        let alto_imagen = this._figura_alto;\n\n        //let abajo = this.pilas.camara.borde_abajo;\n\n        //Obtenemos alto de enemigo\n        //let altoEnemigo = this.pilas.obtener_actor_por_nombre(\"rocaCuadrada\")._figura_alto;\n\n        if (this.x == argumento2) {\n            //if(this.x > 0 &&  this.x < ancho_pantalla)\n            if (this.x > 0) this.x == 0;\n            if (this.x > ancho_pantalla_horizontal - ancho_imagen) this.x == ancho_pantalla_horizontal - ancho_imagen;\n        }\n\n        if (this.y == argumento2) {\n            //if(this.x > 0 &&  this.x < ancho_pantalla)\n            if (this.y > 0) this.y == 0;\n            if (this.y > ancho_pantalla_vertical - alto_imagen) this.y == ancho_pantalla_vertical - alto_imagen;\n        }\n\n\n    }\n\n    getCircleX(radians, radius) {\n        return Math.cos(radians) * radius;\n        this.decir(\"trt\",5);\n        this.pilas.observar(\"math\", this);\n    }\n\n    //console.log(getCircleX(1, 10));\n    // expected output: 5.403023058681398\n\n    //console.log(getCircleX(2, 10));\n    // expected output: -4.161468365471424\n\n    //console.log(getCircleX(Math.PI, 10));\n    // expected output: -10\n\n\n\n\n\n\n}"
            },
