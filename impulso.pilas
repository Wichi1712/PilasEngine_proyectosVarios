                "nombre": "enemigo",
                "codigo": "// @ts-ignore\nclass enemigo extends Actor {\n\n    iniciar() {\n        this.y = 300;\n        this.x = this.pilas.azar(-200, 200);\n    }\n\n    actualizar() {\n        this.y -= 2;\n\n        // si el actor sale de la pantalla por\n        // debajo, se elimina.\n        if (this.y < -300) {\n            this.eliminar();\n        }\n    }\n\n    cuando_colisiona(actor: Actor) {\n\n        if (actor.tiene_etiqueta(\"disparo\")) {\n            // Elimina al enemigo\n            this.eliminar();\n\n            // Elimina al disparo\n            actor.eliminar();\n\n            // Crea una explosi칩n en la misma\n            // posici칩n que el enemigo.\n            var explosion = this.pilas.actores.explosion();\n            explosion.x = this.x;\n            explosion.y = this.y;\n\n            // realiza el movimiento de la c치mara.\n            this.pilas.camara.vibrar(1, 0.2);\n\n            // aumenta el puntaje.\n            //let puntaje = this.pilas.obtener_actor_por_nombre(\"puntaje\");\n            //puntaje.aumentar();\n        }\n    }\n}"
