            {
                "nombre": "canion1",
                "codigo": "// @ts-ignore\nclass canion1 extends Actor {\n\n  velocidad = 5;\n  cuadros_desde_el_ultimo_disparo: number;\n  //seleccionEscena;\n  selector: number;\n\n  iniciar() {\n    this.animacion = \"canon_en_reposo\";\n    this.cuadros_desde_el_ultimo_disparo = 0;\n    //this.seleccionEscena = this.pilas.obtener_actor_por_etiqueta(\"control\");\n    this.selector = 1;\n  }\n\n\n  disparar() {\n    if (this.selector == 1) {\n      let laser = this.pilas.actores.laser();\n      laser.etiqueta = \"disparo\";\n      laser.x = this.x;\n      laser.y = this.y;\n      laser.rotacion = this.rotacion + 90;\n      laser.z = this.z + 1;\n      this.cuadros_desde_el_ultimo_disparo = 0;\n    }\n  }\n\n  //MANTENEMOS UN ANGULO DE DISPARO LIMITADO\n  angulo_disparo() {\n    //GIRO HACIA LA IZQUIERDA SIEMPRE QUE EL ANGULO SEA MENOR A 90 GRADOS\n    if (this.pilas.control.izquierda) {\n      if (this.rotacion < 90) {\n        this.rotacion += this.velocidad;\n        this.animacion = \"canon_girando_izquierda\";\n      }\n\n    }\n\n    //GIRO HACIA LA DERECHA SIEMPRE QUE EL ANGULO SEA MAYOR A -90 GRADOS\n    if (this.pilas.control.derecha) {\n      if (this.rotacion > -90) {\n        this.rotacion -= this.velocidad;\n        this.animacion = \"canon_girando_derecha\";\n      }\n    }\n  }\n  /*\n      cuando_llega_el_mensaje_cambiarSelector(){\n          if(this.selector == 0)this.selector = 1;\n          else if(this.selector == 1)this.selector = 0;\n      }\n  */\n  /*\n      cuando_llega_el_mensaje_cambiarSelector(){\n          if(this.selector == 0)this.selector = 1;\n          if(this.selector == 1)this.selector = 3;\n          if(this.selector == 3)this.selector = 2;\n          if(this.selector == 2)this.selector = 1;\n          if(this.selector == 1)this.selector = 0;\n      }\n      */\n\n  /*\n      cuando_pulsa_tecla(tecla){\n          if (tecla == \"x\") {\n              if(this.selector == 0){\n                  this.selector = 1;\n                  this.pilas.enviar_mensaje_global(\"cambiarSelector\");\n                  }\n              else if(this.selector == 1){\n                  this.selector = 0;\n                  this.pilas.enviar_mensaje_global(\"cambiarSelector\");\n                  }\n          }\n          \n      }\n      */\n\n  cuando_pulsa_tecla(tecla: string) {\n    if (tecla == \"x\") {\n      this.selector++;\n      if (this.selector > 4) this.selector = 1;\n    }\n\n  }\n\n  actualizar() {\n    this.pilas.observar(\"selector1\", this.selector)\n    this.cuadros_desde_el_ultimo_disparo += 1;\n\n    //CONTROL DE DIRECCION DE ANGULO\n    this.angulo_disparo()\n\n    //CONTROL DE DISPARO\n    if (this.pilas.control.espacio && this.cuadros_desde_el_ultimo_disparo > 5) this.disparar();\n    //if (!this.pilas.control.espacio){this.selector = 0;}\n\n    //CAMBIAMOS LA ANIMACION CUANDO GIRAMOS EL CA√ëON\n    //if (this.pilas.control.arriba) {\n    ///////this.avanzar(this.rotacion, this.velocidad);\n    //////this.animacion = \"nave_avanzando\";\n    //}\n    //else {\n    if (!this.pilas.control.izquierda && !this.pilas.control.derecha) this.animacion = \"canon_en_reposo\";\n    //}\n  }\n\n}"
            },
