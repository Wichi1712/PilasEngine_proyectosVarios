            {
                "nombre": "dado4",
                "codigo": "// @ts-ignore\nclass dado4 extends Actor {\n \n    contador;\n    anchoPantalla;\n    altoPantalla;\n\n    iniciar() {\n        this.contador = 0;\n        this.velocidad_x = 2;\n        this.velocidad_y = 2;\n\n        this.anchoPantalla = this.pilas.camara.borde_derecho - this.figura_ancho/2;\n\n        this.altoPantalla = this.pilas.camara.borde_arriba - this._figura_alto/2;\n    }\n\n    recorrido_borde_de_pantalla() {\n        let ancho_pantalla = this.pilas.camara.borde_derecho - this.figura_ancho/2;\n        let alto_pantalla = this.pilas.camara.borde_arriba - this._figura_alto/2;\n\n        //lado derecho\n        if (this.x > ancho_pantalla) {\n            this.x = ancho_pantalla;\n            this.contador = 1;\n        }\n  \n        //arriba\n        if (this.y > alto_pantalla) {\n            this.y = alto_pantalla;\n            this.contador = 2;\n        }\n\n        //lado izquierdo\n        if (this.x < -ancho_pantalla) {\n            this.x = -ancho_pantalla;\n            this.contador = 3;\n        }\n            \n        //abajo\n        if (this.y < -alto_pantalla) {\n            this.y = -alto_pantalla;\n            this.contador = 4;\n        }\n            \n    }\n\n\n    condicional_switch(){\n        let velocidad = 4;\n\n        switch(this.contador){\n            case 0:\n                this.avanzar(0,velocidad);\n            break;\n\n            case 1:\n                this.avanzar(90,velocidad);\n                this.sprite.setTint(0x00ff00);\n            break;\n\n            case 2:\n                this.avanzar(180,velocidad);\n                this.sprite.setTint(0x0000ff);\n            break;\n\n            case 3:\n                this.avanzar(270,velocidad);\n                this.sprite.setTint(0xff0000);\n            break;\n\n            case 4:\n                this.avanzar(0,velocidad);\n                this.sprite.setTint(0xaaaaee);\n            break;\n\n        }\n                \n    }\n\n    debug(){\n        this.pilas.observar(\"contador\", this.contador);\n        this.pilas.observar(\"X\", this.x);\n        this.pilas.observar(\"Y\", this.y);\n        this.pilas.observar(\"Alto\", this.altoPantalla);\n        this.pilas.observar(\"Ancho\", this.anchoPantalla);\n    }\n\n\n    actualizar() {\n        this.recorrido_borde_de_pantalla();\n        this.condicional_switch();   \n        this.debug();\n\n    }\n\n}"
