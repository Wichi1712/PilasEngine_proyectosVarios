                "codigo": "// @ts-ignore\nclass dado3 extends Actor {\n\n        /*--------HACER QUE UN OBJETO GIRE ALREDEDOR DE OTRO----------------\n            Tenemos dos maneras de hacerlo, ambas funcionan igual\n            la unica diferencia es que en el metodo 2 creamos 2 funciones uno para la\n            coordenada X y otro para para coordenada Y.\n        */\n\n    angulo;\n\n    iniciar() {\n        this.angulo = 0;//Iniciamos nuestro actor en el angulo 0\n        this.sprite.setTint(0xff0000);//Este cambio de color no es necesario, solo lo uso para distinguirlo de los dem√°s.\n    }\n\n    actualizar() {\n        //Esta variable es para ambos metodos\n        const objetivo = this.pilas.obtener_actor_por_nombre(\"dado2\");\n\n        //--------------METODO 1 ------------------------\n        /*\n        let longitud = 100;//Distancia entre ambos objetos\n\n        let long_dir_X = longitud * Math.cos(this.angulo);//Nueva coordenada en X\n        let long_dir_Y = longitud * Math.sin(this.angulo);//Nueva coordenada en Y\n        \n        let velocidad = 0.05;\n\n        if (this.angulo >= 6.30) this.angulo = 0;//Volvemos el anugulo a 0\n        //this.pilas.observar(\"angulo dado2\",this.angulo);\n\n        this.x = objetivo.x + long_dir_X;//Actualizamos la posicion X\n        this.y = objetivo.y + long_dir_Y;//Actualizamos laposicion Y\n\n        this.angulo +=velocidad;//Sumamos velocidad al angulo\n        */\n\n        //---------------------METODO 2 --------------------------\n        \n        let longitud = 100;\n        let velocidad = 0.05;\n\n        if (this.angulo >= 6.30) this.angulo = 0;\n\n        this.x = objetivo.x + this.longDirX(longitud,this.angulo);\n        this.y = objetivo.y + this.longDirY(longitud,this.angulo);\n\n        this.angulo +=velocidad;\n        \n    }\n\n    //Funciones usadas para metodo 2\n    longDirX(longitud,direccion){\n        return longitud * Math.cos(direccion);\n    }\n\n    longDirY(longitud,direccion){\n        return longitud * Math.sin(direccion);\n    }\n\n}"
            },
            {
                "nombre": "cabeza",
                "codigo": "// @ts-ignore\nclass cabeza extends Actor {\n\n    giro;\n    velocidad;\n    veloMax;\n    image_angle;\n    hijos;\n\n\n    iniciar() {\n        //VAriables\n        this.giro = 5;\n        this.velocidad = 5;\n        this.veloMax = 11;\n        this.image_angle = 0;\n\n        //Creacion del cuerpo\n\n        this.hijos = 15;//cantidad de cuerpo\n\n        for (let i=1;i < this.hijos;i++)\n        {\n            //let hijooo = this.pilas.clonar_en(\"cuerpoGusano\", this.x, this.y);\n        //let hijo= [i];// = this.pilas.clonar_en(\"cuerpoGusano\", this.x, this.y);\n        \n        let hijo = [i];\n        hijo = this.pilas.clonar_en(\"cuerpoGusano\", this.x + i, this.y + i);\n        \n        //instance_create(x,y,obj_body_player);\n        \n        /* Si i = 1 entonces el padre ser√° el obj_player de lo contrario\n        el padre ser√° hijo - 1*/\n        //let cuerpo = this.pilas.obtener_actor_por_nombre(\"cuerpoGusano\");\n\n        if (i == 1)\n            {\n            //hijo[i].padre = this;//obj_player;\n            }\n        // else{hijo[i].padre = hijo[i-1];}\n        }\n        //fin crear cuerpo\n\n        //for (let fila = 0; fila < 3; fila++) {\n           // for (let columna = 0; columna < 6; columna++) {\n                //this.pilas.clonar_en(\"cuerpoGusano\", -180 + columna * 70, 100 + fila * 40)\n            //}\n        //}\n\n     }\n\n\n    actualizar() {\n        let velocidad = 5;\n\n        if (this.pilas.control.izquierda) {\n            this.x -= velocidad;\n        }\n\n        if (this.pilas.control.derecha) {\n            this.x += velocidad;\n        }\n\n        if (this.pilas.control.arriba) {\n            this.y += velocidad;\n        }\n\n        if (this.pilas.control.abajo) {\n            this.y -= velocidad;\n        }\n\n\n        \n\n    }\n\n}"
